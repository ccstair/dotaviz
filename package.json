{
  "name": "dotaviz_react_redux",
  "version": "1.0.0",
  "description": "This is a data visualization tool to assist draft characters in the videogame dota2.",
  "main": "webpack.config.js",
  "scripts": {
    "test": "test",
    "start": "npm run build && node server/server.js",
    "build": "webpack",
    "commit": "git-cz",
    "seed": "knex --cwd ./server/database/ seed:run",
    "db:migrate:make": "knex --cwd ./server/database/ migrate:make",
    "db:up": "knex --cwd ./server/database/ migrate:latest",
    "db:down": "knex --cwd ./server/database/ migrate:rollback"
  },
  "keywords": [
    "data",
    "visualization",
    "dota2",
    "react",
    "redux",
    "node",
    "express",
    "mysql"
  ],
  "author": "Chris Stair",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.15.3",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-1": "^6.22.0",
    "body-parser": "^1.16.0",
    "bookshelf": "^0.10.3",
    "bookshelf-camelcase": "^1.1.4",
    "dotenv": "^4.0.0",
    "express": "^4.14.1",
    "knex": "^0.12.6",
    "lodash": "^4.17.4",
    "mysql": "^2.13.0",
    "path": "^0.12.7",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.0",
    "redux-promise": "^0.5.3",
    "webpack": "^1.14.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
